<Application x:Class="MamanNet.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindowView.xaml"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/HomepageView.xaml"/>
                <ResourceDictionary Source="Views/Files/AllFilesView.xaml"/>
                <ResourceDictionary Source="Views/Files/DownloadingFilesView.xaml"/>
                <ResourceDictionary Source="Views/Files/UploadingFilesView.xaml"/>
                <ResourceDictionary Source="Views/Files/DownloadedFilesView.xaml"/>
                <ResourceDictionary Source="Views/BrowserView.xaml"/>
            </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
